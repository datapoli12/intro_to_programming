# define the function
def get_grade(score):  
    if score >= 90:
        grade = "A"
    elif score >= 80:
        grade = "B"
    elif score >= 70:
        grade= "C"
    elif score >= 60:
        grade ="D"
    elif score >= 0 and score < 60:  
        grade = "F"
    else:
        grade = "check if the score is correct"
    return grade   
