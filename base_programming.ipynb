def get_water_bill(num_gallons):
   #define the price per 1000 gallons
   price_per_mil = num_gallons/1000
   if num_gallons <= 8000:
        bill = price_per_mil * 5
   elif num_gallons <= 22000:
        bill = price_per_mil * 6
